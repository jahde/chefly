<!-- ============================ Page header =========================== -->

<section id="page-header" class="element-img scrollme" style="background-image: url(<%= image_url("pancakes.jpg") %>)">
    <div class="container">
        <div class="row boost">
            <div class="col-xs-12 col-sm-12 col-md-8 col-md-offset-2 vertical-align text-center">
                <div class="animateme center-me" data-when="exit" data-from="0" data-to="0.6" data-opacity="0" data-translatey="120" style="opacity: 0; transform: translate3d(0px, 120px, 0px) rotateX(0deg) rotateY(0deg) rotateZ(0deg) scale3d(1, 1, 1);">
                    <h2><span><%= @dish.name %></span><p><%= @dish.description %></h2>
                </div>
            </div>
        </div>
    </div>
    <div class="headerfade beige-dk" style="opacity: -1.66666666666667;"></div>
</section>

<!-- ============================ END Header & Logo bar =========================== -->

