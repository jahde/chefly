<section class="container boost">
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-10 col-lg-offset-1">
                    <div class="titleblock text-center">
                        <%= image_tag("logo-orange.png", alt: "logo", height: 40) %><p>&nbsp;<p>
                        <h2>Sign In!</h2>
                        <p>Great things await...</p>
                    </div>
                    
                    <div class="col-md-4"></div>
                    
                    <div class="col-md-4 emailform">
                        <form name="sentMessage" class="well" id="contactForm" novalidate="">
                            <div class="control-group">
                                <div class="controls">
                                    <p>Your E-mail</p>
                                    <input type="text" class="form-control" id="name" required="" data-validation-required-message="Enter Name">
                                <div class="help-block"></div></div>
                            </div>
                            <div class="control-group">
                                <div class="controls">
                                    <p>Your Password</p>
                                    <input type="email" class="form-control" id="email" required="" data-validation-required-message="Enter Email">
                                <div class="help-block"></div></div>
                            </div>
                            <div id="success"> </div>
                            <!-- For success/fail notifications upon clicking 'Send' -->
                            <button type="submit" class="btn btn-default pull-right send">Log In</button>
                        </form>
                        <p>&nbsp;</p>
                    </div>
                    <div class="col-md-4"></div>

                    
                </div>
            </div>
        </section>